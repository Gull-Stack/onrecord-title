    .pitch-hero .container { position: relative; z-index: 1; }
    .pitch-hero h1 { color: var(--white); margin-bottom: var(--space-sm); }
    .pitch-hero .subtitle { color: rgba(255,255,255,0.85); font-size: var(--text-lg); margin-bottom: var(--space-lg); }
