    .pitch-footer a { color: var(--primary); font-weight: var(--font-semibold); }

    /* --- Tag Pills --- */
    .tag { display: inline-block; padding: 2px 10px; border-radius: 20px; font-size: var(--text-xs); font-weight: var(--font-semibold); }
    .tag-critical { background: rgba(220,53,69,0.1); color: var(--error); }
