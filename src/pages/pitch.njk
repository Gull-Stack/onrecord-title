    /* --- Responsive --- */
    @media (max-width: 600px) {
      .pitch-section { padding: var(--space-lg); }
      .comparison { font-size: var(--text-xs); }
      .comparison th, .comparison td { padding: var(--space-sm); }
    }
  </style>
</head>
