    .score-ring .number { font-size: var(--text-4xl); font-weight: var(--font-extrabold); line-height: 1; }
    .score-ring .out-of { font-size: var(--text-xs); color: var(--text-light); }
    .score-ring .grade { font-size: var(--text-sm); font-weight: var(--font-bold); margin-top: var(--space-xs); }

    .score-breakdown { display: flex; flex-direction: column; gap: var(--space-md); }
    .score-bar-item label { display: flex; justify-content: space-between; font-size: var(--text-sm); font-weight: var(--font-medium); margin-bottom: var(--space-xs); }
    .score-bar { height: 8px; background: var(--gray-200); border-radius: 4px; overflow: hidden; }
    .score-bar .fill { height: 100%; border-radius: 4px; transition: width 0.8s ease; }

    /* --- Issue Cards --- */
    .issue-list { display: flex; flex-direction: column; gap: var(--space-md); }
    .issue {
      display: flex; gap: var(--space-md); padding: var(--space-lg);
