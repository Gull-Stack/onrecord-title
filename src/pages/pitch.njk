    .issue.critical .severity { background: var(--error); }
    .issue.warning .severity { background: var(--warning); }
    .issue.info .severity { background: var(--primary); }
    .issue h4 { font-family: var(--font-body); margin-bottom: var(--space-xs); font-size: var(--text-base); }
    .issue p { font-size: var(--text-sm); color: var(--text-light); margin-bottom: 0; }

