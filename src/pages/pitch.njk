    h1 { font-size: clamp(1.75rem, 4vw, 2.75rem); font-weight: var(--font-bold); }
    h2 { font-size: clamp(1.25rem, 3vw, 1.75rem); font-weight: var(--font-bold); }
    h3 { font-size: var(--text-xl); font-weight: var(--font-semibold); }
