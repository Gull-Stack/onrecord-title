    .comparison { width: 100%; border-collapse: collapse; margin-top: var(--space-lg); }
    .comparison th, .comparison td { padding: var(--space-md); text-align: left; font-size: var(--text-sm); border-bottom: 1px solid var(--gray-200); }
    .comparison th { font-weight: var(--font-semibold); color: var(--text-dark); background: var(--gray-50); }
